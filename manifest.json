{
  "name": "Video Speed",
  "description": "Changes the video speed with just two clicks.",
  "version": "1.0",
  "manifest_version": 3,
  "author": "Gustavo Lopes",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "onload.js"
    ]
  }],
  "action": {
    "default_title": "Video Speed",
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  }
}